apiVersion: "cilium.io/v2"
kind: CiliumNetworkPolicy
metadata:
  name: "news-feed-service-rule"
spec:
  description: "The port 8093 is blocked"
  endpointSelector:
    matchLabels:
      role: frontend     #change your role here
  ingress:
    - fromCIDR:
        - {}       
      toPorts:
        - ports:
            - port: "8093"
              protocol: TCP
